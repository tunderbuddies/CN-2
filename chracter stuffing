#include<stdio.h> 
#include<string.h> 
int main()
{
char a[20],b[20];
int n;
printf("enter the string");
scanf("%s",a);
strcpy(b,"dle");
n=strlen(a); 
printf("dlestx"); 
for(int i=0;i<n;i++){
int count =0;
int p=i;
for(int j=0;j<3;j++){
if(a[i]==b[i]){
count=count+1; 
i++;
}
}
if(count!=3){ 
i=p;}
if(count==3){ 
printf("dledle");
}
else{
printf("%c",a[i]);
}
}
printf("dleetx");
printf("%s",a);
return 0;
}

